{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.apollo.js",
  "scripts": {
    "apollo-init": "cd ./dist/; touch apollo.js",
    "clean:apollo": "rimraf dist/apollo.js",
    "apollo:start-server": "nodemon --watch dist/apollo.js --exec \"node dist/apollo.js\"",
    "apollo:build-server": "webpack --config webpack.apollo.js --watch --progress",
    "apollo-setup": "npm run clean:apollo && npm run apollo-init",
    "apollo": "npm run apollo-setup && npm-run-all --parallel apollo:*",
    "dev-init": "mkdir dist; cd ./dist; touch ssr.js; touch bundle.js",
    "clean:dev": "rimraf dist/ssr.js && rimraf dist/bundle.js",
    "dev:server": "nodemon --watch dist/ssr.js --exec \"node dist/ssr.js\"",
    "dev:build-client": "webpack --config webpack.client.js --watch --progress",
    "dev:build-server": "webpack --config webpack.ssr.js --watch --progress",
    "dev": "NODE_ENV=development && npm run build",
    "prebuild": "npm run clean:dev && npm run dev-init",
    "build": "npm-run-all --parallel dev:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/mongoose": "^5.10.3",
    "@types/node-sass": "^4.11.1",
    "@types/passport": "^1.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.1.7",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.0",
    "source-map-loader": "^2.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.3",
    "typescript-plugin-css-modules": "^3.2.0",
    "webpack": "^5.20.2",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "apollo-datasource-mongodb": "^0.3.0",
    "apollo-server-express": "^2.19.2",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^3.2.0",
    "cross-fetch": "^3.0.6",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "mongoose": "^5.11.15",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-parallax": "^3.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "session": "^0.1.0",
    "swiper": "^6.4.11"
  }
}
